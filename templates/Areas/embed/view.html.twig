<section class="area-embed" style="margin-top:20px;">
    <div class="row">
        {% for i in 1..2 %}
        <div class="col-sm-6">
            {% for i in pimcore_iterate_block(pimcore_block('contents_' ~ i)) %}
                <div class="embed">
                    {{
                        pimcore_embed('socialContent', {
                            width: 500
                        })
                    }}
                </div>
            {% endfor %}
        </div>
        {% endfor %}

    </div>
</section>

